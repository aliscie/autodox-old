{
  "canisters": {
    "backend": {
      "candid": "canisters/backend/backend.did",
      "package": "backend",
      "type": "rust"
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "frontend": {
        "entrypoint": "frontend/index.html"
      },
      "source": [
        "frontend/dist"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8510",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}