type BackendError = variant { UnknownError };
type Data = record {
  id : text;
  content : text;
  name : text;
  children : vec text;
  parent : opt text;
};
type Result = variant { Ok; Err : BackendError };
service : () -> {
  create_file : (text, text, text, vec text, opt text) -> (Result);
  read_file : () -> (vec Data) query;
}